{
	"name": "presentation-parzival",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"docker:start": "docker pull existdb/existdb:6.2.0 && docker run -d -p 8081:8080 -p 8444:8443 --name teipublisher existdb/existdb:6.2.0",
		"installXar": "wget https://dhbern.github.io/parzival-static-api/parzival-0.2.xar -O parzival-0.2.xar && xst package install parzival-0.2.xar && rm parzival-0.2.xar",
		"docker:stop": "docker stop teipublisher && docker rm teipublisher",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@existdb/xst": "^3.3.0",
		"@skeletonlabs/skeleton": "^3.2.2",
		"@skeletonlabs/skeleton-svelte": "^1.5.3",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/enhanced-img": "^0.8.4",
		"@sveltejs/kit": "^2.47.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@types/d3": "^7.4.3",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.5",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sander": "^0.6.0",
		"svelte": "^5.41.2",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"vite": "^7.1.11"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.7.4",
		"@fortawesome/fontawesome-free": "^7.1.0",
		"@skeletonlabs/floating-ui-svelte": "^0.3.9",
		"@tailwindcss/vite": "^4.1.15",
		"@types/openseadragon": "^5.0.1",
		"@vincjo/datatables": "^2.6.6",
		"d3": "^7.9.0",
		"estree-walker": "^3.0.3",
		"minisearch": "^7.2.0",
		"openseadragon": "^5.0.1",
		"slugify": "^1.6.6"
	}
}
